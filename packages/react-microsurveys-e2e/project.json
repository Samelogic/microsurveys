{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react-microsurveys-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/react-microsurveys-e2e/cypress.json",
        "devServerTarget": "react-microsurveys:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "react-microsurveys:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/react-microsurveys-e2e/**/*.{js,ts}"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["react-microsurveys"]
}
